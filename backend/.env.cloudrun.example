# Cloud Run Configuration Example
# Copy to .env and modify as needed

# Server settings
DEBUG=false
PORT=8080

# CORS settings - Update with your frontend URL
CORS_ORIGINS=https://your-frontend.vercel.app,https://your-domain.com

# Upload settings
MAX_FILE_SIZE_MB=10
MAX_BATCH_SIZE=10

# Rate limiting
RATE_LIMIT_PER_DAY=5

# AI Model settings (optimized for production)
PLATE_YOLO_MODEL=yolov8s-license-plate.pt
FACE_DETECTION_CONFIDENCE=0.5
PLATE_DETECTION_CONFIDENCE=0.5
PLATE_MIN_CONFIDENCE=0.4
PLATE_YOLO_IMGSZ=1280
DEBUG_PLATE_DETECTION=false

# Default blur settings
DEFAULT_BLUR_INTENSITY=80
